{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    }
  },
  "plugins": [
    "import"
  ],
  "rules": {
    "no-console": 1,
    "import/no-extraneous-dependencies": 1,
    "import/no-unresolved": 1
  }
}
